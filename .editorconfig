# For more information about the properties used in
# this file, please see the EditorConfig documentation:
# http://editorconfig.org/

root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = tab
insert_final_newline = false


[*.md]
trim_trailing_whitespace = false

[*.{rb,ru}]
indent_style = space
indent_size = 2